# {{ ansible_managed }}
[Unit]
Description=uWSGI instance to serve multinet
After=network.target

[Service]
Type=simple
User={{ ansible_user_id }}
Group={{ ansible_user_id }}
WorkingDirectory={{ multinet_server_path }}/multinet
ExecStart={{ ansible_env.HOME }}/.local/bin/uwsgi --ini {{ multinet_server_path }}/multinet/app.ini

[Install]
WantedBy=multi-user.target
